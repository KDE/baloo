#: indexerstate.cpp:20
#, kde-format
msgid "Suspended"
msgstr "முடக்கப்பட்டுள்ளது"

#: indexerstate.cpp:23
#, kde-format
msgid "Initial Indexing"
msgstr "முதல் அட்டவணையாக்கம்"

#: indexerstate.cpp:26
#, kde-format
msgid "Indexing new files"
msgstr "புதிய கோப்புகள் அட்டவணையாக்கப்படுகின்றன"

#: indexerstate.cpp:29
#, kde-format
