msgstr "Vishesh Handa"

#: main.cpp:57
#, kde-format
msgid "The command to execute"
msgstr "O comando a executar"

#: main.cpp:59
#, kde-format
msgid "Print the status of the indexer"
msgstr "Imprime o estado do sistema de indexação"

#: main.cpp:60
#, kde-format
msgid "Enable the file indexer"
msgstr "Activar a indexação de ficheiros"

#: main.cpp:61
#, kde-format
msgid "Disable the file indexer"
msgstr "Desactivar a indexação de ficheiros"

#: main.cpp:62
#, kde-format
msgid "Remove the index database"
msgstr "Remover a base de dados do índice"

#: main.cpp:63
#, kde-format
msgid "Suspend the file indexer"
msgstr "Suspender a indexação de ficheiros"

#: main.cpp:64
#, kde-format
msgid "Resume the file indexer"
msgstr "Prosseguir a indexação de ficheiros"

#: main.cpp:65
#, kde-format
msgid "Check for any unindexed files and index them"
msgstr "Procurar por ficheiros não indexados e indexá-los"

#: main.cpp:66
#, kde-format
msgid "Index the specified files"
msgstr "Indexar os ficheiros indicados"

#: main.cpp:67
#, kde-format
msgid "Forget the specified files"
msgstr "Esquecer os ficheiros indicados"

#: main.cpp:68
#, kde-format
msgid "Modify the Baloo configuration"
msgstr "Modificar a configuração do Baloo"

#: main.cpp:69
#, kde-format
msgid "Monitor the file indexer"
msgstr "Monitorizar a indexação de ficheiros"

#: main.cpp:70
#, kde-format
msgid "Display the disk space used by index"
msgstr "Mostrar o espaço em disco usado pelo índice"

#: main.cpp:71
#, kde-format
msgid "Display files which could not be indexed"
msgstr "Mostrar os ficheiros que não puderam ser indexados"

#: main.cpp:74
#, kde-format
msgctxt ""
"Format to use for status command, %1|%2|%3 are option values, %4 is a CLI "
"command"
msgid ""
"Output format <%1|%2|%3>.\n"
"The default format is \"%1\".\n"
"Only applies to \"%4\""
msgstr ""
"Formato de saída <%1|%2|%3>.\n"
"O formato por omissão é o \"%1\".\n"
"Só se aplica ao \"%4\""

#: main.cpp:80
#, kde-format
msgid "format"
msgstr "formato"

#: monitorcommand.cpp:54
#, kde-format
