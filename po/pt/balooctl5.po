#: main.cpp:78
#, kde-format
msgid "Enable the file indexer"
msgstr "Activar a indexação de ficheiros"

#: main.cpp:79
#, kde-format
msgid "Disable the file indexer"
msgstr "Desactivar a indexação de ficheiros"

#: main.cpp:80
#, kde-format
msgid "Start the file indexer"
msgstr "Iniciar a indexação de ficheiros"

#: main.cpp:81
#, kde-format
msgid "Stop the file indexer"
msgstr "Parar a indexação de ficheiros"

#: main.cpp:82
#, kde-format
msgid "Restart the file indexer"
msgstr "Reiniciar a indexação de ficheiros"

#: main.cpp:83
#, kde-format
