#, kde-format
msgid "A filter must be provided"
msgstr "Полоише бояд қайд карда шавад"

#: configcommand.cpp:220
#, kde-format
msgid "%1 is not in list of exclude filters"
msgstr "%1 берун аз рӯйхати полоишҳои истисношуда мебошад"

