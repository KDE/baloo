msgstr "Käivitatav käsk"

#: main.cpp:59
#, kde-format
msgid "Print the status of the indexer"
msgstr "Indekseerija oleku näitamine"

#: main.cpp:60
#, kde-format
msgid "Enable the file indexer"
msgstr "Failide indekseerija lubamine"

#: main.cpp:61
#, kde-format
msgid "Disable the file indexer"
msgstr "Failide indekseerija keelamine"

#: main.cpp:62
#, kde-format
msgid "Remove the index database"
msgstr "Indekseerimise andmebaasi eemaldamine"

#: main.cpp:63
#, kde-format
msgid "Suspend the file indexer"
msgstr "Failide indekseerija ajutine peatamine"

#: main.cpp:64
#, kde-format
msgid "Resume the file indexer"
msgstr "Failide indekseerija töö jätkamine"

#: main.cpp:65
#, kde-format
msgid "Check for any unindexed files and index them"
msgstr "Indekseerimata failide otsimine ja nende indekseerimine"

#: main.cpp:66
#, kde-format
msgid "Index the specified files"
msgstr "Määratud failide indekseerimine"

#: main.cpp:67
#, kde-format
msgid "Forget the specified files"
msgstr "Määratud failide kõrvalejätmine"

#: main.cpp:68
#, kde-format
msgid "Modify the Baloo configuration"
msgstr "Baloo seadistuse muutmine"

#: main.cpp:69
#, kde-format
msgid "Monitor the file indexer"
msgstr "Failide indekseerija jälgimine"

#: main.cpp:70
#, kde-format
msgid "Display the disk space used by index"
msgstr "Indeksi kasutatud kettaruumi näitamine"

#: main.cpp:71
#, kde-format
msgid "Display files which could not be indexed"
msgstr "Failide näitamine, mida ei saa indekseerida"

