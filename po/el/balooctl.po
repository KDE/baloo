msgstr "Εκτύπωση της κατάστασης της εφαρμογής δεικτοδότησης"

#: main.cpp:77
#, kde-format
msgid "Enable the file indexer"
msgstr "Ενεργοποίηση της εφαρμογής δεικτοδότησης"

#: main.cpp:78
#, kde-format
msgid "Disable the file indexer"
msgstr "Απενεργοποίηση της εφαρμογής δεικτοδότησης"

#: main.cpp:79
#, kde-format
msgid "Start the file indexer"
msgstr "Εκκίνηση της εφαρμογής δεικτοδότησης"

#: main.cpp:80
#, kde-format
msgid "Stop the file indexer"
msgstr "Τερματισμός της εφαρμογής δεικτοδότησης"

#: main.cpp:81
#, kde-format
msgid "Restart the file indexer"
msgstr "Επανεκκίνηση της εφαρμογής δεικτοδότησης"

#: main.cpp:82
#, kde-format
msgid "Suspend the file indexer"
msgstr "Αναστολή της εφαρμογής δεικτοδότησης"

#: main.cpp:83
#, kde-format
msgid "Resume the file indexer"
msgstr "Συνέχιση της εφαρμογής δεικτοδότησης"

#: main.cpp:84
#, kde-format
msgid "Check for any unindexed files and index them"
msgstr "Έλεγχος για μη δεικτοδοτημένα αρχεία και δεικτοδότησή τους"

#: main.cpp:85
#, kde-format
msgid "Index the specified files"
msgstr "Να δεικτοδοτηθούν τα συγκεκριμένα αρχεία"

#: main.cpp:86
#, kde-format
msgid "Forget the specified files"
msgstr "Να μη συμπεριληφθούν τα συγκεκριμένα αρχεία"

#: main.cpp:87
#, kde-format
msgid "Modify the Baloo configuration"
msgstr "Τροποποίηση της διαμόρφωσης του Baloo"

